{
	"license": "MIT",
	"name": "clevercanyon/clevercanyon-php-chalk",
	"description": "A tool to style terminal output.",
	"authors": [
		{
			"name": "Martin Pettersson",
			"email": "martin_pettersson@outlook.com",
			"homepage": "http://martindotcom.se",
			"role": "Developer"
		},
		{
			"name": "CLEVER CANYON",
			"homepage": "https://clevercanyon.com",
			"role": "organization"
		}
	],
	"prefer-stable": true,
	"minimum-stability": "dev",
	"config": {
		"process-timeout": 0,
		"optimize-autoloader": true
	},
	"require": {
		"php": "^7.4 || ^8.0"
	},
	"autoload": {
		"psr-4": {
			"Clever_Canyon\\Chalk\\": "./src/classes"
		}
	},
	"extra": {
		"props": {
			"brand": {
				"data": {
					"slug": "clevercanyon"
				},
				"aws": {
					"s3": {
						"bucket": "clevercanyon",
						"cdn_domain": "cdn.clevercanyon.com"
					}
				}
			}
		},
		"props-dev": {
			"dev_json_map": {
				"brand": {
					"aws": {
						"s3": {
							"hash_hmac_key_path": "clevercanyon.aws.s3.hash_hmac_key"
						}
					}
				}
			}
		}
	}
}
